package test2;

public class InversePairs {
	public int InversePairs(int [] array) {
		if(array == null || array.length<=1) return 0;
		
		return 0;
	}
	private int mergeSort(int [] array,int lo, int hi) {
		 if(lo>=hi) return 0;
		 int mid = (lo+hi)/2;
		 
		 return mergeSort(array,lo,mid-1)+mergeSort(array,mid,hi)+merge(array,lo,hi);
	}
	private int merge(int[] array, int lo, int hi) {
		if(lo>=hi) return 0;
		int num = 0;
		int mid = (lo+hi)/2;
		int i = mid-1;
		while(i>=lo) {
			
		}
	}
}
